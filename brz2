
properties([buildDiscarder(logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '', numToKeepStr: '')), pipelineTriggers([[$class: 'PeriodicFolderTrigger', interval: '1d']])])

